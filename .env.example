# Environment variables for the Untron frontend
# Copy this file to .env and fill in the appropriate values

# Node environment (e.g., development, production, test)
NODE_ENV=development

# Backend API URL (required)
REACT_APP_BACKEND_URL=https://api.example.com

# WalletConnect Project ID (required)
# Obtain this from your WalletConnect dashboard
REACT_APP_WALLETCONNECT_PROJECT_ID=abcdef1234567890

# USDC contract address on Base network (required)
REACT_APP_USDC_BASE_ADDRESS=0x1234567890123456789012345678901234567890

# Untron Intents contract address on Base network (required)
REACT_APP_UNTRON_INTENTS_BASE_ADDRESS=0x0987654321098765432109876543210987654321